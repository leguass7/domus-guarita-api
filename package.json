{
  "name": "domus-guarita-api",
  "version": "2.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "release": "standard-version --no-verify --skip.tag --release-as patch -a",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig-build.json && tscpaths -p tsconfig-build.json -s ./src -o ./dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "cross-env NODE_ENV=testing jest --coverage --forceExit --detectOpenHandles",
    "lint": "eslint ./src --ext js --ext ts --fix",
    "s": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify swagger.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.36",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.0",
    "jest-mock-process": "^1.4.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-useragent": "^1.0.15",
    "helmet": "^5.1.0",
    "http-terminator": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.2",
    "request-ip": "^2.1.3",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "yamljs": "^0.3.0"
  }
}
